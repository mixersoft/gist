// REFERENCE: Newtonsoft.Json.dll

using Newtonsoft.Json.Linq;
using Newtonsoft.Json;
using System.Console;
using System.IO;

{
	def data =
		using (reader = StreamReader(OpenStandardInput()))
			JObject.Parse(reader.ReadToEnd());
	using (file = JsonTextWriter(StreamWriter(OpenStandardOutput())))
	{
		file.Formatting  = Formatting.Indented;
		file.Indentation = 1;
		file.IndentChar  = '\t';
		data.WriteTo(file);
	}
}
